function applyTableOption(c,d){var e=$("#smlDeleteListForm"),b="";$('input:checkbox[name$="row_checkbox"]:checked').each(function(){b=$(this).val();e.append('<input type="hidden" name="listName" value="'+b+'" />')});if(b){if(d=="delete"){$("#deleteConfirmDialog #deleteSubmitButton").click(function(){e.submit()});$("#deleteConfirmDialog").dialog("open")}}else{var a=$("#noCheckboxesSelectedMessage").val();alert(a);return false}}function smlcancel(){$("#deleteConfirmDialog").dialog("close")}function toggleSMLCheckbox(a){$('input:checkbox[name$="row_checkbox"]').each(function(){$(this).prop("checked",a.checked)});$('input:checkbox[name$="row_edit_checkbox"]').each(function(){$(this).prop("checked",a.checked)});$('input:checkbox[name$="selectAllCheckbox"]').each(function(){$(this).prop("checked",a.checked)})}function showEditView(b,a){$("#row_view_"+a).hide();$("#row_edit_"+a).show();$("#sml_"+a).on("change keypress paste",function(c){$("#sml_"+a).val($("#sml_"+a).val().replace(" ","_"))});window.onresize=function(c){showWindowedOverlay(a)};showWindowedOverlay(a)}function showRowView(a){$("#row_edit_"+a).hide();$("#row_view_"+a).show();hideWindowedOverlay(a)}function showWindowedOverlay(e){var d=$("#overlay-top");var c=$("#overlay-right");var a=$("#overlay-bottom");var k=$("#overlay-left");var b=$("#row_edit_"+e);var f=b.position();var g=b.height();var i=$("#smlTableOuter").width();var j=$("#smlTableOuter").position().left;var h=j-(f.left-j);d.css({top:0,left:0,width:$(document).width(),height:f.top}).show();c.css({top:f.top,left:i+h,width:$(document).width()-(i+h),height:g}).show();a.css({top:f.top+g,left:0,width:$(document).width(),height:$(document).height()-f.top-g}).show();k.css({top:f.top,left:0,width:f.left,height:g}).show()}function hideWindowedOverlay(e){var d=$("#overlay-top");var c=$("#overlay-right");var a=$("#overlay-bottom");var k=$("#overlay-left");var b=$("#row_edit_"+e);var f=b.position();var g=b.height();var i=$("#smlTableOuter").width();var j=$("#smlTableOuter").position().left;var h=j-(f.left-j);d.css({top:0,left:0,width:$(document).width(),height:f.top}).hide();c.css({top:f.top,left:i+h,width:$(document).width()-i+h,height:g}).hide();a.css({top:f.top+g,left:0,width:$(document).width(),height:$(document).height()-f.top-g}).hide();k.css({top:f.top,left:0,width:f.left,height:g}).hide()}function isValidSmlName(a){var c=/(.*)([<>&\"])+(.*)/;var b=new String(a);if(b.match(c)){return false}return true}function validateAndSaveChanges(b){var a=true;var d="";d=$("#sml_"+b).val();d=$.trim(d);if(d==null||d.length==0){$("#row_error_"+b).html($("#saveSmlNameMissingMessage").val());$("#row_error_"+b).show();a=false}else{if(!isValidSmlName(d)){$("#row_error_"+b).html($("#smlNameInvalidMessage").val());$("#row_error_"+b).show();a=false}else{if(dupName=findDuplicateSmlName(d,b)){$("#row_error_"+b).html('"'+dupName+'"'+$("#smlNameDupMessage").val());$("#row_error_"+b).show();a=false}else{$("#sml_error_"+b).hide()}}}if(a){var c=$("#smlEditForm_"+b);c.submit();return true}showWindowedOverlay(b)}function findDuplicateSmlName(c,b){var a="";$('input:text[name="listName"]').each(function(){var e=$(this).val();e=$.trim(e);if(e==c){var f=$(this).attr("id");var d=f.substring(4);if(d!=b){a=$("#sml_"+d).val()}}});return a}function getCount(a){$.ajax({type:"GET",url:a,dataType:"text",success:function(c){if(c!="ERROR"&&c>0){var b=$("#open_manage").show()}},error:function(){}})}function submitAndRetrieveSml(a,b){if(typeof b!=="undefined"){$("#smlOpenListForm_"+a).attr("action",function(c,d){return d+b})}$("#smlOpenListForm_"+a).submit()};
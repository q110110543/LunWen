(function(c){var d=c.fn.select2.amd.require("select2/defaults");c.extend(d.defaults,{dropdownPosition:"auto"});var a=c.fn.select2.amd.require("select2/dropdown/attachBody");var b=a.prototype._positionDropdown;a.prototype._positionDropdown=function(){var h=c(window);var r=this.$dropdown.hasClass("select2-dropdown--above");var g=this.$dropdown.hasClass("select2-dropdown--below");var o=null;var i=this.$container.offset();i.bottom=i.top+this.$container.outerHeight(false);var f={height:this.$container.outerHeight(false)};f.top=i.top;f.bottom=i.top+f.height;var q={height:this.$dropdown.outerHeight(false)};var m={top:h.scrollTop(),bottom:h.scrollTop()+h.height()};var l=m.top<(i.top-q.height);var n=m.bottom>(i.bottom+q.height);var k={left:i.left,top:f.bottom};var p=this.$dropdownParent;if(p.css("position")==="static"){p=p.offsetParent()}var e=p.offset();k.top-=e.top;k.left-=e.left;var j=this.options.get("dropdownPosition");if(j==="above"||j==="below"){o=j}else{if(!r&&!g){o="below"}if(!n&&l&&!r){o="above"}else{if(!l&&n&&r){o="below"}}}if(o=="above"||(r&&o!=="below")){k.top=f.top-e.top-q.height}if(o!=null){this.$dropdown.removeClass("select2-dropdown--below select2-dropdown--above").addClass("select2-dropdown--"+o);this.$container.removeClass("select2-container--below select2-container--above").addClass("select2-container--"+o)}this.$dropdownContainer.css(k)}})(window.jQuery);
function saveToML(a){$.ajax({type:"GET",url:a,dataType:"text",async:false,success:function(d){if(d!="ERROR"){var c=parseInt(d);var b=$("#maxMarkedList").val();if(c<b-1){$("#smlUpToHelpText").show();$("#smlOneMoreHelpText").hide();$("#smlZeroMoreHelpText").hide()}if(c==b-1){$("#smlOneMoreHelpText").show();$("#smlUpToHelpText").hide();$("#smlZeroMoreHelpText").hide()}if(c==b){$("#smlZeroMoreHelpText").show();$("#smlUpToHelpText").hide();$("#smlOneMoreHelpText").hide()}$("#currentMLCount").val(c);$("#saveToMLDialog").dialog("open")}},error:function(){}})};